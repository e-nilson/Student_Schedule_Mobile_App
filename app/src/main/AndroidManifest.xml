<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.c196">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.C196">
        <receiver
            android:name=".UI.MyReceiver"
            android:enabled="true"
            android:exported="true"></receiver>
        <activity
            android:name=".UI.AssessmentDetailList"
            android:exported="false"
            android:label="Assessment Details"
            android:parentActivityName=".UI.AssessmentList" />
        <activity
            android:name=".UI.AssessmentList"
            android:exported="false"
            android:label="Assessments"
            android:parentActivityName=".UI.MainActivity" />
        <activity
            android:name=".UI.CourseDetailList"
            android:exported="false"
            android:label="Course Details"
            android:parentActivityName=".UI.CourseList" />
        <activity
            android:name=".UI.CourseList"
            android:exported="false"
            android:label="Courses"
            android:parentActivityName=".UI.MainActivity" />
        <activity
            android:name=".UI.TermDetailList"
            android:exported="false"
            android:label="Term Details"
            android:parentActivityName=".UI.TermList" />
        <activity
            android:name=".UI.TermList"
            android:exported="false"
            android:label="Terms"
            android:parentActivityName=".UI.MainActivity" />
        <activity
            android:name=".UI.MainActivity"
            android:exported="true"
            android:label="Home"
            android:parentActivityName=".UI.LoginActivity" />
        <activity
            android:name=".UI.LoginActivity"
            android:label="Login"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>